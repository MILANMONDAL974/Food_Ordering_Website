<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Loginserve</servlet-name>
        <servlet-class>Finalpack.Loginserve</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Signup</servlet-name>
        <servlet-class>Finalpack.Signup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminlogin</servlet-name>
        <servlet-class>Finalpack.adminlogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminregister</servlet-name>
        <servlet-class>Finalpack.adminregister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DelUser</servlet-name>
        <servlet-class>Finalpack.DelUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DelAdmin</servlet-name>
        <servlet-class>Finalpack.DelAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>contact</servlet-name>
        <servlet-class>Finalpack.contact</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DelContact</servlet-name>
        <servlet-class>Finalpack.DelContact</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>update_address</servlet-name>
        <servlet-class>Finalpack.update_address</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>Finalpack.logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Loginserve</servlet-name>
        <url-pattern>/Loginserve</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Signup</servlet-name>
        <url-pattern>/Signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminlogin</servlet-name>
        <url-pattern>/adminlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminregister</servlet-name>
        <url-pattern>/adminregister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DelUser</servlet-name>
        <url-pattern>/DelUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DelAdmin</servlet-name>
        <url-pattern>/DelAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>contact</servlet-name>
        <url-pattern>/contact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DelContact</servlet-name>
        <url-pattern>/DelContact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>update_address</servlet-name>
        <url-pattern>/update_address</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
